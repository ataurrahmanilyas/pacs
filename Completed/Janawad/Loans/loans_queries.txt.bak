INSERT INTO loan SELECT
	admission_no,
	loan_type,
	product,
	loan_no,
	crop_purpose,
	ledger_folio_no,
	scheme,
	STR_TO_DATE(sanction_date,'%d-%m-%Y'),
	sanction_amount,
	loan_period_in_months,
	STR_TO_DATE(due_date,'%d-%m-%Y'),
	roi,
	penal_roi,
	roi_after_due_date,
	repayment_mode,
	gestation_period,
	STR_TO_DATE(first_installment_due_date,'%d-%m-%Y'),
	installment_amount,
	dpn_no,
	la_no,
	lc_no,
	lc_date,
	dccb_no,
	outstanding_principal_rs,
	outstanding_interest_rs
FROM
	loan_raw; 


